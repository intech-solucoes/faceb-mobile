    "build:config-dev": "./node_modules/.bin/cpy configs/config.dev.json . --rename=config.json",
    "build:config-test": "./node_modules/.bin/cpy configs/config.test.json . --rename=config.json",
    "build:config-prod": "./node_modules/.bin/cpy configs/config.prod.json . --rename=config.json",
